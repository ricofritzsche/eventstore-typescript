{
  "name": "aggregateless-eventstore",
  "version": "0.1.0",
  "description": "A TypeScript implementation of an aggregateless eventstore",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "dev": "ts-node src/index.ts",
    "example": "ts-node src/examples/banking/cli.ts",
    "example:banking": "ts-node src/examples/banking/cli.ts",
    "simple": "ts-node simple.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "prepack": "npm run build"
  },
  "keywords": [
    "eventstore",
    "eventsourcing",
    "aggregateless",
    "typescript",
    "events"
  ],
  "author": "Rico Fritzsche",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "dotenv": "^17.0.1",
    "pg": "^8.11.0",
    "uuid": "^11.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ricofritzsche/eventstore-typescript"
  },
  "bugs": {
    "url": "https://github.com/ricofritzsche/eventstore-typescript/issues"
  },
  "homepage": "https://github.com/ricofritzsche/eventstore-typescript#readme"
}
